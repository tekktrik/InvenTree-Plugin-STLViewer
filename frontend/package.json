    "@mantine/notifications": ">= 8.2.7",
    "@react-three/drei": "latest",
    "@react-three/fiber": "latest",
    "@types/three": "latest",
    "only": "^0.0.2",
    "react": ">= 19.1.1",
    "react-dom": ">= 19.1.1",
    "three": "latest"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.0",
    "@lingui/cli": "latest",
    "@lingui/macro": "latest",
    "@lingui/swc-plugin": "latest",
    "@lingui/vite-plugin": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitejs/plugin-react-swc": "latest",
    "globals": "^15.14.0",
    "typescript": "~5.6.2",
